Config { font = "-*-terminus-*-*-*-*-12-*-*-*-*-*-*-u"
, bgColor      = "#000000"
, fgColor      = "#758C5D"
, position =  TopW L 90
, lowerOnStart = True
, commands = [
		Run Cpu ["-t","<total>"]  50
	, Run Memory ["-t","<usedratio>%"] 50
	, Run Swap ["-t", "<usedratio>"] 50
	, Run Com "/home/karl/bin/batterybar.sh" [""] "battery" 100
	, Run BatteryP ["BAT1"] ["-t", "<left>"] 300
	, Run Com "date" ["+'%H:%M'"] "cmdlinetime" 600
	, Run Com "date" ["+'%a %d of %B'"] "cmdlinedate" 3000
	, Run Com "/home/karl/bin/uptimebar.sh" [""] "cmdlineuptime" 600
	, Run Com "/home/karl/bin/mpd.sh" [""] "mpd" 10
	, Run StdinReader ]
	, sepChar      = "%"
	, alignSep     = "}{"
	, template = " %StdinReader% }{ <fc=#95C95F>CPU: </fc>%cpu% | <fc=#95C95F>MEM: </fc>%memory% | <fc=#95C95F>SWAP: </fc>%swap% | <fc=#95C95F>BAT: </fc>%battery% | <fc=#95C95F>%cmdlinetime%</fc> on %cmdlinedate%"
  }
